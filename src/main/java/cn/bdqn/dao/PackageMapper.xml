<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.dao.PackageMapper">
    <select id="getPackageList" resultType="packages">
        select * from package;
    </select>
    <select id="getByPackagePaId" resultType="packages">
        select * from package where paId=#{paId};
    </select>
</mapper>